{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "5.10.1",
  "applications": {
    "gecko": {
      "id": "shrunked@mllr.pl",
      "strict_min_version": "128.0"
    }
  },
  "author": "Geoff Lankow, memeller",
  "homepage_url": "https://github.com/memeller/shrunked",
  "icons": {
    "16": "content/icon16.png",
    "32": "content/icon32.png",
    "64": "content/icon64.png"
  },
  "default_locale": "en",
  "background": {
    "type": "module",
    "scripts": ["background.js"]
  },
  "permissions": ["compose", "storage", "tabs", "menus"],
  "options_ui": {
    "page": "content/config.xhtml"
  },
  "experiment_apis": {
    "shrunked": {
      "schema": "api/shrunked.json",
      "parent": {
        "scopes": ["addon_parent"],
        "paths": [["shrunked"]],
        "script": "api/shrunked.js"
      }
    }
  }
}
